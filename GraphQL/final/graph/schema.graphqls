# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  name:String!
}

type Query {
  todos: [Todo!]!
}

type newid{
  id:ID!
}

input NewTodo {
 id:ID!
 name:String!
}

input id{
  id:ID!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
  updateTodo(input: NewTodo!): Todo!
  deleteTodo(input: id!): newid!
}
